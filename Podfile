# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

target 'Hubba' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Hubba
  pod ‘Alamofire’
  pod ‘SwiftyJSON’
  pod ‘SDWebImage’
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'FBSDKShareKit'

  target 'HubbaTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Nimble'
    pod 'Quick'
  end

end
